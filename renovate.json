  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "rangeStrategy": "bump",
  "prHourlyLimit": 0,
  "ignoreDeps": ["node", "next", "eslint-config-next"],
  "packageRules": [
    {
      "allowedVersions": "18",
      "matchPackageNames": ["@types/node"]
    }
  ]
}
